# Medusa Core Configuration
DATABASE_URL=postgresql://user:password@localhost:5432/senshop_production
REDIS_URL=redis://localhost:6379
MEDUSA_ADMIN_ONBOARDING_TYPE=nextjs
STORE_CORS=https://yourdomain.com,https://www.yourdomain.com
ADMIN_CORS=https://yourdomain.com,https://admin.yourdomain.com
AUTH_CORS=https://yourdomain.com,https://admin.yourdomain.com

# Security
JWT_SECRET=your-production-jwt-secret-min-32-chars
COOKIE_SECRET=your-production-cookie-secret-min-32-chars

# Supabase Configuration
SUPABASE_URL=https://your-project-id.supabase.co
SUPABASE_ANON_KEY=your-supabase-anon-key
SUPABASE_SERVICE_ROLE_KEY=your-supabase-service-role-key
SUPABASE_ARTWORK_BUCKET=artworks
SUPABASE_DIGITAL_BUCKET=digital-products
SUPABASE_THUMBNAIL_BUCKET=thumbnails

# Printful Configuration
PRINTFUL_API_TOKEN=your-printful-api-token
PRINTFUL_WEBHOOK_SECRET=your-printful-webhook-secret
PRINTFUL_ENVIRONMENT=production

# Stripe Configuration
STRIPE_SECRET_KEY=sk_live_your-stripe-secret-key
STRIPE_PUBLISHABLE_KEY=pk_live_your-stripe-publishable-key
STRIPE_WEBHOOK_SECRET=whsec_your-stripe-webhook-secret

# Medusa Publishing
MEDUSA_PUBLISHABLE_KEY=pk_your-generated-publishable-key

# Optional: Monitoring & Analytics
SENTRY_DSN=your-sentry-dsn
NODE_ENV=production